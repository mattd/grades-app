    "webpack-dev-middleware": "^1.11.0",
    "webpack-hot-middleware": "^2.18.0"
  },
  "scripts": {
    "build": "npm run clean && NODE_ENV=production webpack --config config/webpack/production.js",
    "build:integration": "FIREBASE_ENV=integration npm run build && npm run package",
    "build:production": "FIREBASE_ENV=production npm run build && npm run package",
    "build:staging": "FIREBASE_ENV=staging npm run build && npm run package",
    "clean": "rm -rf ./src/static ./public",
    "lint": "./node_modules/.bin/eslint src/js",
    "package": "mkdir public && cp -R src/index.html src/static public",
    "preflight": "npm run lint && npm run test",
    "start": "source scripts/set-timestamp.sh && source scripts/set-commit.sh && PORT=3000 node server.js",
    "test": "NODE_PATH=src/js ava",
    "test:watch": "npm test -- --watch"
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "source": [
      "src/js/**/*.{js,jsx}"
    ],
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
